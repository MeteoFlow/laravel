{
    "name": "meteoflow/laravel",
    "description": "Laravel integration for MeteoFlow Weather API SDK",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "weather", "api", "meteoflow", "forecast", "sdk"],
    "homepage": "https://github.com/meteoflow/laravel",
    "authors": [
        {
            "name": "MeteoFlow Team",
            "email": "support@meteoflow.com"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "meteoflow/php": "^1.3.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "MeteoFlow\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeteoFlow\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MeteoFlow\\Laravel\\MeteoFlowServiceProvider"
            ],
            "aliases": {
                "MeteoFlow": "MeteoFlow\\Laravel\\Facades\\MeteoFlow"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
